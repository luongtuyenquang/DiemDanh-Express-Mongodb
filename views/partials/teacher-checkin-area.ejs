<% if(studentAt.length > 0) { %>
<b>Danh sách sinh viên điểm danh</b>
<div class="table-responsive">
  <table class="table text-center table-bordered table-hover  ">
    <thead class="thead-light">
      <tr class="d-flex">
        <th class="col-1">STT</th>
        <th class="col-2">Họ Và tên</th>
        <th class="col-4">Thời gian điểm danh</th>
        <th class="col-2">Tên hoạt động</th>
      </tr>
    </thead>
    <tbody>
      <% for(var i=0;i<studentAt.length;i++){ %>
        <tr class="d-flex">
          <th class="col-1"><%- i+1 %></th>
          <th class="col-2"><%- studentAt[i].HoVaTen %></th>
          <th class="col-4">Thời gian điểm danh</th>
          <th class="col-2"><%- act[i].TenSuKien %></th>
        </tr>
        <% } %>
      
    </tbody>
  </table>
</div>
<% } %>