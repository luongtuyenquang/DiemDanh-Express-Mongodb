
<script>
 $(document).ready(function() {
    $(function () {$('[data-toggle="tooltip"]').tooltip()});
  });
  
function submitAT(c){
    $('#submitDiemDanh').modal('hide');
    $.ajax({
    type:'POST',
    url:`/createDiemDanh?c=${c}`,
    success:function(checkDiemDanh){
      if(checkDiemDanh.length > 0) {
        $('#showInput').html("<form>" + `<input type="text">` + "</form>")
        $.notify( 'Bạn đã điểm danh !')
      }
      else{
        $('#showInput').html(function() {
          const result = `<input type="text" />`
          return result;
        })
        $.notify( 'Điểm danh thành công');
        //setTimeout(`window.location.href="/student-diemdanh"`,6000);
      }
  

    }
})
}
  </script>
<!-- Notify -->
<% if(mess&&mess!=''){ %>
  <script>
    $.notify("<%- mess %>", "success");
  </script>  
<% } %>

